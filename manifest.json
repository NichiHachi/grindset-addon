{
    "manifest_version": 2,
    "name": "Grindset",
    "version": "1.0",
    "description": "Stop procrastinating, happiness is temporary but knowledge is eternal.",
    "icons": {
        "48": "icons/grindset_32.png"
    },
    "permissions": ["activeTab","https://leetcode.com/","storage"],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["grindset.js"]
        }
    ],
    "browser_action": {
        "default_popup": "popup.html"
    }
}